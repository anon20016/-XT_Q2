@using Entities;
@using BAL;

@{
    Layout = "~/Pages/Layouts/_MainLayout.cshtml";
}

@{
    IUserLogic userManagerLogic = new UserLogic();
    userManagerLogic.LoadData();
    var temp = userManagerLogic.GetAll();
}

    <table>
        <col width="100" />
        <col width="450" />
        <col width="250" />
        @foreach (var item in temp)
        {
            <tr>
                <td>
                    @item.Id
                </td>
                <td>
                    @item.Name
                </td>
                <td>
                    @item.DateOfBirth
                </td>
            </tr>
        }
    </table>

